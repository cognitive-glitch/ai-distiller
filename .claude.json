{
  "mcpServers": {
    "aid": {
      "command": "~/ai-distiller/mcp-npm/bin/aid-mcp",
      "args": [],
      "env": {
        "AID_ROOT": "~/ai-distiller",
        "AID_CACHE_DIR": "~/.cache/aid"
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "zen": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "zen-mcp-server",
        "python",
        "server.py"
      ]
    }
  }
}